{
    "env": {
        "node": true,
        "browser": true,
        "jquery": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-useless-constructor": 1,
        "camelcase": 1,
        "no-useless-escape": 1
    }
}
